    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.10",
    "restify": "^6.0.0",
    "restify-router": "^0.5.1",
    "rimraf": "^2.4.5",
    "ts-node": "^3.3.0",
    "typeorm": "0.1.0-alpha.42",
    "typescript": "^2.9.2"
  }
